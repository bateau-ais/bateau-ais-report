# Configuration NATS
NATS_URL=tcp://127.0.0.1:4222
NATS_MAX_RECONNECT_ATTEMPTS=10
NATS_RECONNECT_DELAY_SECONDS=2

# Configuration du module
MODULE_NAME=bateau-ais-module
MODULE_VERSION=0.1.0
LOG_LEVEL=INFO

# Topics NATS
TOPIC_INPUT=ais.raw.*
TOPIC_OUTPUT=ais.processed
TOPIC_ALERTS=alerts

# Paramètres métier (exemple)
SPEED_THRESHOLD=50
ANOMALY_DETECTION_ENABLED=true
